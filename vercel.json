{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "DATABASE_URL": "@database_url",
    "GEMINI_API_KEY": "@gemini_api_key"
  },
  "regions": ["gru1"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
